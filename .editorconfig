# http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = "utf-8"
end_of_line = lf

[*.js]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.js]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true


[*.json, *.lesshintrc]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true